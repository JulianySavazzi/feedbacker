{
    "regions": [
        "gru1"
    ],
    "builds": [
        {
            "src": "/public/index.php",
            "use": "vercel-php@0.6.2"
        }
    ],
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/public/build/assets/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        },
        {
            "src": "/login",
            "methods": ["POST"],
            "headers": { "X-Authorization": "true", "Cache-Control": "max-age=0", "Access-Control-Allow-Credentials": "true", "Access-Control-Request-Headers": "content-type", "Access-Control-Request-Method": "POST", "Content-Type": "application/json"},
            "status": 419
        },
        {
            "src": "/register",
            "methods": ["POST"],
            "headers": { "X-Authorization": "true", "Cache-Control": "max-age=0", "Access-Control-Allow-Credentials": "true", "Access-Control-Request-Headers": "content-type", "Access-Control-Request-Method": "POST", "Content-Type": "application/json"},
            "status": 419
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)", "destination": "/"
        }
    ],
    "env": {
        "APP_NAME": "Laravel",
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "VERCEL_DEMO_MODE": "true",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "CACHE_STORE": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp",
        "NODE_PATH": "node"
    }
}
